[color]
  branch = auto
  diff = auto
  status = auto
  ui = true
[color "branch"]
  current = "#076678 bold"
  local = "#076678"
  plain = "#ff00ff"
  remote = "#8f3f71"
  upstream = "#8f3f71"
[color "diff"]
  commit = "#ff00ff"
  context = "#ff00ff"
  frag = "#3c3836 #ebdbb2 bold"
  func = "#3c3836 nobold"
  meta = "#427b58"
  new = "#79740e"
  old = "#9d0006"
  plain = "#7c6f64"
  whitespace = "#af3a0e reverse"
[color "diff-highlight"]
  newHighlight = "#79740e bold ul"
  newNormal = "#79740e"
  oldHighlight = "#9d0006 bold ul"
  oldNormal = "#9d0006"
[color "status"]
  added = "#79740e"
  branch = "#af3a03 bold"
  changed = "#9d0006"
  header = "#076678"
  nobranch = "#d6540e"
  unmerged = "#d65d0e bold"
  untracked = "#7c6f64"
[commit]
  verbose = true
[core]
  editor = /opt/homebrew/bin/vim
  excludesfile = ~/.gitignore.global
  pager = diff-so-fancy | less --tabs=4 -RFX
[credential]
  helper = cache --timeout=3600
[format]
  pretty = %C("#7c6f64")%h %C(reset)%C("#bdae93")â†’ %C("#3c3836")%s%C(reset) %C("#076678")%aN %C("#a89984")%cd%C("#8f3f71")%d
[github]
  user = jarednorman
[init]
  defaultBranch = main
[log]
  date = relative
  decorate = short
[merge]
  conflictstyle = diff3
[push]
  default = current
[rebase]
  autosquash = true
[user]
  name = Jared Norman
  email = jared@super.gd
